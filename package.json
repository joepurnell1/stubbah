    "clean": "node config/cleanFolder.js",
    "lint": "eslint 'src/*' --fix"
  },
  "author": "Wei",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "exec": "npm run start",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
